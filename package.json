{
  "name": "uni-translate-tool",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "x <895740313@qq.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build-mac": "vue-tsc --noEmit && vite build && electron-builder build --mac",
    "build-win": "vue-tsc --noEmit && vite build && electron-builder build --win",
    "build-linux": "vue-tsc --noEmit && vite build && electron-builder build --linux",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.3",
    "autoprefixer": "^10.4.16",
    "electron": "^27.1.3",
    "electron-builder": "^24.6.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tree-kill": "^1.2.2",
    "typescript": "^5.1.6",
    "vite": "^5.0.4",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "axios": "^1.6.3",
    "body-parser": "^1.20.2",
    "electron-store": "^8.1.0",
    "element-plus": "^2.4.4",
    "express": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "remixicon": "^4.0.1",
    "vue-router": "^4.2.5"
  }
}
